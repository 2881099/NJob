    <AssemblyName>NJob</AssemblyName>
    <PackageId>NJob</PackageId>
    <RootNamespace>NJob</RootNamespace>
    <Version>1.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/2881099/NJob</PackageProjectUrl>
